pi = 3.14
halfPi = pi / 2


# FIRST THIRD
for i <- 3 to 20 {
a = i * 7
arc(60 10 a halfPi pi / 1.4 )
}

# SECOND THIRD
for i <- 3 to 20 {
a = i * 7
arc(60 10 a pi / 1.3 pi )
}

# LAST THIRD
for i <- 3 to 20 {
a = i * 7
arc(60 10 a pi * 1.05 pi * 1.275 )
}



line(60 -11 60 -123)
line(47 -6 -23 -94)
line(45 -4 -39.5 -78.5)
line(39 13 -71 31)
line(39 10 -73 10)
line(46 26 -27 111)